                    if(Trigger.oldMap.get(t.id).Call_Attempt_Status__c != Trigger.newMap.get(t.id).Call_Attempt_Status__c &&
                       t.Call_Attempt_Status__c == 'No Contact' && t.WhoId != null && t.WhoId.getSObjectType().getDescribe().getName() == 'Lead')
                    {
                       tList.add(t); 
                    }
                    if(Trigger.oldMap.get(t.id).Call_Attempt_Status__c != Trigger.newMap.get(t.id).Call_Attempt_Status__c &&
                       t.Call_Attempt_Status__c != 'No Contact' && t.WhoId != null && t.WhoId.getSObjectType().getDescribe().getName() == 'Lead'){
                       // &&
                       //t.Sub_Dispositions__c != 'Ringing' && t.Sub_Dispositions__c != 'Not Reachable' && t.Sub_Dispositions__c != 'Call Busy' &&
                       //t.Sub_Dispositions__c != 'Swicthed Off'
                       tList2.add(t); 
                    }
                }
                system.debug('no contact list:: '+tList.size());
                system.debug('no contact list2:: '+tList2.size());
                if(!tList.isEmpty()){
                    TaskManagementServices.incrementNoContactCounter(tList);
                }
                if(!tList2.isEmpty()){
                    TaskManagementServices.setZeroNoContactCounter(tList2);
                }
                
           //sending a task to ozonetel scheduler API 
           Boolean statusCode = False;
           if(Call_Ozonetel_Manual_API__c.getValues('API Calling') != Null){
            if(Call_Ozonetel_Manual_API__c.Scheduler_API_to_be_Called__c != Null){ 
                 statusCode = Call_Ozonetel_Manual_API__c.getValues('API Calling').Scheduler_API_to_be_Called__c;}}
                 system.debug('statusCode :: '+ statusCode);
                
                for(Task t:Trigger.new){
                    system.debug('Inside Trigger on update ::: '+t.subject);
                    if(statusCode == True){
                        if(t.Next_Action_Date__c != Trigger.oldMap.get(t.Id).Next_Action_Date__c){ 
                         if(t.Status == 'Completed' &&  t.Next_Action_Date__c != null)           
                           tIdSet.add(t.id);
                         system.debug('Task set :: '+tIdSet);
                        }
                    } 
             	 }
                 /* Calling Follow Up (Ozontel Scheduler) API */ 
                  if(!tIdSet.IsEmpty())
                     callOzonetelSchedulerAPI.scheduleCall(tIdSet);
            }
        }    
