    public demandViewNew() {
        string towerexceptionstr = system.label.TowerException_For_Demand; // added by vamsi on 12/07/2020
        list<string> towerexceptionlst = new list<string>();   // added by vamsi on 12/07/2020
        towerexceptionlst = towerexceptionstr.split(',');   // added by vamsi on 12/07/2020
        towerexception = new set<string>();     // added by vamsi on 12/07/2020
        isSiniya = false;
        for(string var: towerexceptionlst){     // added by vamsi on 12/07/2020
            towerexception.add(var);    // added by vamsi on 12/07/2020
        }   // added by vamsi on 12/07/2020    
        letterH = false;
        demandDate = System.today();
        dwList = new List <DemandManagementServices.DemandWrapper>();
        Map<String, Integer> projChargeSeq;
        sgstGT = 0;
        cgstGT = 0;    
        AmountGT = 0;     
        percentCV = 0; // Added by Neha on 25/3/19
        percentCVStr = ''; // Added by Neha on 25/3/19
        invoiceDueDate = '';
        invoiceNumber = '';     
        leglEntyFavoring  = '';
        leglEntyEscrowAccount  = '';
        leglEntyIBAN  = '';
        leglEntySWIFTCode = '';
        leglEntyBankName = '';     
        Ref = '';
        totalOutstanding = 0;  ///// Added by Neha on 4/7/19
        totalOutstandingString = '0'; ///// Added by Neha on 4/7/19
        totalAmountDue = '0';  ///// Added by Neha on 4/7/19
        totalOutstandingAmount = '0';  ///// Added by Neha on 4/7/19
        totalOutstandingTax = '0';  ///// Added by Neha on 4/7/19
        currentOutstandingString = '0'; ///// Added by Neha on 4/7/19
        totalAmtPaid = '0'; ///// Added by Neha on 30/7/19
        totalPercentDue = '';
        previousPercentDue = '';
        perdue=0;
        try {

            if (ApexPages.currentPage().getParameters().containsKey('previewId')){
                previewId = (Id) ApexPages.currentPage().getParameters().get('previewId');
            }
            if (ApexPages.currentPage().getParameters().containsKey('forMilestone')){
                forMilestone = (Id) ApexPages.currentPage().getParameters().get('forMilestone');
            }
            if (ApexPages.currentPage().getParameters().containsKey('mode')){
                mode = ApexPages.currentPage().getParameters().get('mode');
            }
            if (ApexPages.currentPage().getParameters().containsKey('invoiceDueDate')){
                invoiceDueDate = ApexPages.currentPage().getParameters().get('invoiceDueDate');
            }
            if (ApexPages.currentPage().getParameters().containsKey('invoiceNumber')){
                invoiceNumber = ApexPages.currentPage().getParameters().get('invoiceNumber');
            }
            if (ApexPages.currentPage().getParameters().containsKey('letterHead')) {
                letterHead = ApexPages.currentPage().getParameters().get('letterHead');
                if (letterHead == '1') {
                    letterH = true;
                }
            }
            
            if (previewid != null) {
                Customer = [SELECT Id, Name, Primary_Applicant_Name__c, Primary_Applicant_Email__c, PrimaryMobileNumber__c, Unit__r.Name, Unit__r.Tower__r.Name,
                            Unit__r.Project__r.Name,Opportunity__r.Name,OwnerId,Owner.Email,Unit__c,Opportunity__c,Unit__r.Tower__r.Cluster__r.Name,
                            Amount_in_USD__c,Opportunity__r.Amount_in_USD__c,Unit__r.Project__r.Project_Image__c,Unit__r.Configuration__c,
                            Unit__r.Project__c,Opportunity__r.Permanent_Address__c,Unit__r.Tax_Rate_Basic__c, Unit__r.Tower__c, Unit__r.Tower__r.RDS_Company_Code__r.Name,
                            Unit__r.Tower__r.Address__c,Unit__r.Chargeable_Area__c, Opportunity__r.Other_Applicants_Name__c,
                            Unit__r.Tower__r.RDS_Company_Code__r.Account_Number_Flat_Cost__c,Project__r.Name
                            FROM Booking__c WHERE 
                            Unit__c =: previewId AND 
                            Status__c = 'Processed' AND 
			                Current_Status__c = 'Audit Accepted'];
    
                bAmount = Customer.Opportunity__r.Amount_in_USD__c;
                if(Customer.Project__r.Name.contains('Siniya')){
                    isSiniya = true;
                }else{
                    isSiniya = false;
                }
                if(Customer.Unit__r.Name != null && String.isNotBlank(Customer.Unit__r.Name)){
                    Ref = Customer.Unit__r.Name + ', ';  
                }
                if((Customer.Unit__r.Tower__r.Name != null) && (String.isNotBlank(Customer.Unit__r.Tower__r.Name)) && (!towerexception.contains(Customer.Unit__r.Tower__r.Name))){
                    Ref += Customer.Unit__r.Tower__r.Name + ', ';
                }
                if(Customer.Unit__r.Tower__r.RDS_Company_Code__r.Name != null && String.isNotBlank(Customer.Unit__r.Tower__r.RDS_Company_Code__r.Name)){
                    Ref += Customer.Unit__r.Tower__r.RDS_Company_Code__r.Name + ', ';
                }
                if(Customer.Unit__r.Tower__r.Address__c != null && String.isNotBlank(Customer.Unit__r.Tower__r.Address__c)){
                    Ref += Customer.Unit__r.Tower__r.Address__c;
                }
                
                leObj = InventoryCostServices.getLegalEntityDetails(Customer.Unit__c,Customer.Unit__r.Project__c,Customer.Unit__r.Tower__c);
                if(leObj != null){              
                    leglEntyFavoring  = leObj.Favouring__c;
                    leglEntyEscrowAccount  = String.valueOf(leObj.Account_Number_Flat_Cost__c);
                    leglEntyIBAN  = leObj.IBAN__c;
                    leglEntySWIFTCode = leObj.MICR_Code_Flat_Cost__c;
                    leglEntyBankName = leObj.Bank_Name_Flat_Cost__c ;
                }
                
                Map <Id, List <DemandManagementServices.DemandWrapper>> customerDemandWrapperMap  = new Map <Id, List <DemandManagementServices.DemandWrapper>>();
                customerDemandWrapperMap = DemandManagementServices.searchSpecificDemands(Customer.Unit__r.Tower__r.Name,
                                                                                         Customer.Unit__r.Project__r.Name, Customer.Opportunity__r.Name, Customer.Unit__r.Name, null,System.Today(), null, null, forMilestone);                           
                dwList = customerDemandWrapperMap.get(previewId);
                projChargeSeq = InventoryCostServices.getProjectChargesSequence(Customer.Unit__r.Project__r.Name, Customer.Unit__r.Tower__c);
                Decimal Seq = 0;
                if(projChargeSeq != null){
                    Seq = projChargeSeq.get('Basic');
                }
                Decimal USDtotalAmout =0;
                if(dwList != null && !dwList.isEmpty()){ 
                    for (DemandManagementServices.DemandWrapper d1: dwList) {
                        sgstGT += d1.ptw.overallTax1D;
                        cgstGT += d1.ptw.overallTax2D;
                        if(d1.ptw.overallAmtD> 0){
                            AmountGT+= d1.ptw.overallAmtD;
                        }
                        if(d1.dm.Total_Amount_with_Tax_Demanded__c != null && d1.dm.Total_Amount_Paid__c != null){
                            perdue =  (d1.dm.Total_Amount_with_Tax_Demanded__c + d1.dm.Total_Amount_Paid__c).setScale(0);
                        }
                        else if(d1.dm.Total_Amount_with_Tax_Demanded__c == null && d1.dm.Total_Amount_with_Tax_Demanded__c != null){
                            perdue =  (d1.dm.Total_Amount_Paid__c).setScale(0);
                        }
                        else if(d1.dm.Total_Amount_with_Tax_Demanded__c != null && d1.dm.Total_Amount_with_Tax_Demanded__c == null){
                            perdue =  (d1.dm.Total_Amount_with_Tax_Demanded__c).setScale(0);
                        }
                        else{
                            perdue = 0;
                        }
                        
                        //// Changed by Neha on 8/11/19
                        if(d1.pm.Invoice_Date__c != null){
                            invoiceDate = d1.pm.Invoice_Date__c.format();
                        }
                        else if (d1.dm.Invoice_Date__c != null){
                            invoiceDate = d1.dm.Invoice_Date__c.format();  
                        }
                        if(d1.pm.get('Charge'+Seq+'Percentage__c') != null) {
                            percentCV += ((Decimal)d1.pm.get('Charge'+Seq+'Percentage__c')).setScale(2); // Added by Neha on 25/3/19 
                        }
                        else if (d1.dm.Payment_Milestones__r.get('Charge'+Seq+'Percentage__c') != null){
                            percentCV += ((Decimal)d1.dm.Payment_Milestones__r.get('Charge'+Seq+'Percentage__c')).setScale(2);
                        }
                    }
                }
                System.debug('===USDtotalAmout'+USDtotalAmout);
                Decimal USDtotalAmount = AmountGT/3.6725;
                cgstGTStr = InventoryCostServices.InFormatR(cgstGT);
                AmountGTStr = InventoryCostServices.InFormatR(AmountGT);
                USDamountstr = InventoryCostServices.InFormatR(USDtotalAmount);
                percentCVStr = percentCV + ' %'; // Added by Neha on 25/3/19
                
                ////////////// Added by Vikas for previous Dues  //////////////////////
                
                Booking__c Booking = [SELECT Id, Quotation__C, Quotation__r.Name, Quotation__r.Total_Agreement_Value_Bucket__c,Booking_Date__c,                
                                      Quotation__r.SGST_on_Total_Agreement_Value_Bucket__c,OwnerId, Quotation__r.CGST_on_Total_Agreement_Value_Bucket__c,
                                      TotalAmountBalance__c, TotalTaxBalance__c, Total_On_Account_Money__c, Total_Due__c                
                                      FROM Booking__c where Opportunity__c = :Customer.Opportunity__c and Unit__c = :Customer.Unit__c limit 1];
                
                if (Booking != null) {
                    bookingDate = Booking.Booking_Date__c.Date();
                    totalAmtPaid = InventoryCostServices.InFormatR(Booking.Total_On_Account_Money__c);
                    CV = Booking.Quotation__r.Total_Agreement_Value_Bucket__c;
                    CVStr = InventoryCostServices.InFormatR(CV);            
                    TaxOnCV = Booking.Quotation__r.SGST_on_Total_Agreement_Value_Bucket__c + Booking.Quotation__r.CGST_on_Total_Agreement_Value_Bucket__c;
                    TaxOnCVStr = InventoryCostServices.InFormatR(TaxOnCV);                
                    TotalCVStr = InventoryCostServices.InFormatR(CV + TaxOnCV);                                   
                }      
            }
        } catch (Exception e) {
            ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.ERROR, 'Error in PDF Demand Not Sent To Customer. Please check demand Manually render as pdf : Time : '+System.Now()+ ' - Error :'+e.getMessage() + ' - Line number :' +e.getLineNumber() + ' Stack Trace :' +e.getStackTraceString()));
        }
    } 

