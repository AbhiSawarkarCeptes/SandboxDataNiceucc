public class SendDemandMail{
    public string unitId{get;set;}
    public string invoiceDueDate{get;set;}
    public string invoiceNumber{get;set;}
    public string milestoneId{get;set;}
    public List<Unit__c> uList{get;set;}
    public string milestoneName{get;set;}
    public string unitName{get;set;}
    public string buildingName{get;set;}
    public string custName{get;set;}
    public Date dueDate {get;set;}
    public string project {get;set;}
    public Boolean isSiniya {get;set;}
       
    public sendDemandMail(){
        milestoneName = '';
        unitName = '';
        buildingName = '';
        custName = '';
        dueDate = null;
        project = '';
        
        try {
            if (ApexPages.currentPage().getParameters().containsKey('unitId')){
                unitId = ApexPages.currentPage().getParameters().get('unitId');
            }
            if (ApexPages.currentPage().getParameters().containsKey('invoiceDueDate')){
                invoiceDueDate =  ApexPages.currentPage().getParameters().get('invoiceDueDate');
            }
            if (ApexPages.currentPage().getParameters().containsKey('invoiceNumber')){
                invoiceNumber =  ApexPages.currentPage().getParameters().get('invoiceNumber');
            }
            if (ApexPages.currentPage().getParameters().containsKey('milestoneId')){
                milestoneId =  ApexPages.currentPage().getParameters().get('milestoneId');    
            }
            
            System.debug('milestoneId::: ' + milestoneId);
            uList = new List<Unit__c>([select id,name,Booking__c,Customers__c,Booking__r.Primary_Applicant_Name__c,Project__r.name,Booking__r.OwnerId,
                                    Tower__r.Cluster__r.name,Tower__r.name, Booking__r.owner.Name,Booking__r.owner.email,Booking__r.owner.Phone,Booking__r.Project__r.Name
                                    from Unit__c where id=:unitId and Booking__c != null and Customers__c != null]);  
            List<Payment_Milestones__c> pm = [select id, name, Milestone_Name__c from Payment_Milestones__c where id = :milestoneId];    
            System.debug('pm::: ' + pm.size() + ' & :: ' + pm);
            if(uList[0].Project__r.Name.contains('Siniya')){
                isSiniya = true;
            }else{
                isSiniya = false;
            }
            if(pm != null && !pm.isEmpty()){
                milestoneName = pm[0].Milestone_Name__c; 
            }        
            System.debug('String duedate:: ' + invoiceDueDate);
            if(invoiceDueDate != null && invoiceDueDate != '')
                dueDate = Date.parse(invoiceDueDate);
            System.debug('duedate::: ' + dueDate);
                
            if(uList != null && !uList.isEmpty()){
                custName = uList[0].Booking__r.Primary_Applicant_Name__c;
                unitName = uList[0].Tower__r.name + '-' + uList[0].name;
                buildingName = uList[0].Project__r.name + '-' + uList[0].Tower__r.Cluster__r.name;
                project = uList[0].Booking__r.Project__r.Name;
            }             
        } catch (Exception e) {
        }
    }
    
    
