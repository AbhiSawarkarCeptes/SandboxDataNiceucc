<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Broker_name_mandatory_on_not_reg</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    ISPICKVAL(Broker_category__c, &apos;Not Registered&apos;),
    ISBLANK(Broker_Name__c)
)</errorConditionFormula>
    <errorMessage>Enter Broker Name</errorMessage>
</ValidationRule>
