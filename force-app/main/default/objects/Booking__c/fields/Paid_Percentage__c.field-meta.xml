<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Paid_Percentage__c</fullName>
    <formula>If(Agreement_Value__c &gt; 0 &amp;&amp; Total_On_Account_Money__c &gt; 0,
(Total_On_Account_Money_With_Registration__c)/(Agreement_Value__c + Tax_on_Purchase_Price__c),
0)</formula>
    <label>Paid Percentage</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
