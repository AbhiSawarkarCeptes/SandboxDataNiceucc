<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Quatation_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISCHANGED(Quotation__c ) ,
 $Profile.Name &lt;&gt; &apos;System Administrator&apos;)</errorConditionFormula>
    <errorDisplayField>Quotation__c</errorDisplayField>
    <errorMessage>Quotation can not be changed after creation of booking</errorMessage>
</ValidationRule>
