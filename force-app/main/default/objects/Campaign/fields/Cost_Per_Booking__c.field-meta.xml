<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cost_Per_Booking__c</fullName>
    <formula>IF( 
AND(HierarchyNumberOfWonOpportunities &lt;&gt; 0, NOT(ISNULL(HierarchyNumberOfWonOpportunities))), 
HierarchyActualCost / HierarchyNumberOfWonOpportunities , 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Cost Per Booking</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
