<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Lead_New_Channel_Partner_Phone</fullName>
    <active>false</active>
    <errorConditionFormula>AND(PRIORVALUE(New_Channel_Partner_Phone__c) != null, 
