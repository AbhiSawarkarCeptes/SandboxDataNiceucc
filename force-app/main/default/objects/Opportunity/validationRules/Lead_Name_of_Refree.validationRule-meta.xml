<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Lead_Name_of_Refree</fullName>
    <active>false</active>
    <errorConditionFormula>AND(PRIORVALUE(Name_of_Refree__c) != null, 
ISCHANGED(Name_of_Refree__c), OR($Profile.Name != &quot;Admin&quot;), 
$Profile.Name != &quot;System Administrator&quot;)</errorConditionFormula>
    <errorDisplayField>Name_of_Refree__c</errorDisplayField>
    <errorMessage>You cannot change Name of Refree, please contact your administrator</errorMessage>
</ValidationRule>
