<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>New_meetings_can_not_have_completed_stat</fullName>
    <active>true</active>
    <errorConditionFormula>And(IsNew(),isPickval(Meeting_Status__c , &apos;Completed&apos;))</errorConditionFormula>
    <errorMessage>Meeting status can not be selected as completed on meeting creation.</errorMessage>
</ValidationRule>
