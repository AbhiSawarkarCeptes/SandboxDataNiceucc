<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Do_not_convert_if_lead_score_is_less</fullName>
    <active>false</active>
    <errorConditionFormula>Lead_Score__c  &lt;= 3  &amp;&amp;   IsConverted == TRUE</errorConditionFormula>
    <errorMessage>You cannot convert a lead if lead score is less than 3</errorMessage>
</ValidationRule>
