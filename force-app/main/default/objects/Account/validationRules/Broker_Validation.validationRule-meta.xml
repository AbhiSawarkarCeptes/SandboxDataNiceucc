<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Broker_Validation</fullName>
    <active>false</active>
    <errorConditionFormula>AND( NOT (ISBLANK(Name_as_per_passport__c)), NOT (ISBLANK( Name_as_per_trade_license__c) ))</errorConditionFormula>
    <errorMessage>Cannot add both the values of field &quot;Name as per passport&quot; and &quot;Name as per trade license&quot; simultaneously.</errorMessage>
</ValidationRule>
